<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/lib/layuimini/lib/layui-v2.7.5/css/layui.css" media="all">
    <link rel="stylesheet" href="/lib/layuimini/css/public.css" media="all">
    <link rel="stylesheet" href="/lib/layuimini/lib/font-awesome-4.7.0/css/font-awesome.min.css" />
    <style>
        .layui-btn:not(.layui-btn-lg ):not(.layui-btn-sm):not(.layui-btn-xs) {
            height: 34px;
            line-height: 34px;
            padding: 0 8px;
        }
        .layui-btn+.layui-btn {
            margin-left: 0px !important;
        }
        .layui-table tr:hover {
            background-color: #fffacd !important;
        }
    </style>
</head>
<body>
<div class="layuimini-container">
        <div class="layuimini-main">
            @RenderBody()
        </div>
    </div>
    <script src="/lib/jquery/jquery.min.js" charset="utf-8"></script>
    <script src="/lib/layuimini/lib/layui-v2.7.5/layui.min.js" charset="utf-8"></script>
    <script src="/lib/layuimini/js/lay-config.js?v=2.0.0" charset="utf-8"></script>
    <script src="/lib/axios/axios.min.js" charset="utf-8"></script>
    <script src="/js/site.js" charset="utf-8"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
